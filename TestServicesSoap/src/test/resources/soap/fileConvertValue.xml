    <soap:Header>
        <ser:Header>
            <ser:Username>%s</ser:Username>
            <ser:Password>?</ser:Password>
            <ser:Tracer>?</ser:Tracer>
        </ser:Header>
    </soap:Header>
    <soap:Body>
        <ser:ConvertRealTimeValue>
            <ser:From>%s</ser:From>
            <ser:To>%s</ser:To>
            <ser:Amount>%s</ser:Amount>
        </ser:ConvertRealTimeValue>
    </soap:Body>
</soap:Envelope>